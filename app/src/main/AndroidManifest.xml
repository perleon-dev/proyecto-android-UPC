<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fast_service">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.Fast_Service">
        <activity
            android:name=".CancelarPedidoActivity"
            android:exported="false" />
        <activity
            android:name=".VisualizarEstadoPedidoActivity"
            android:exported="false" />
        <activity
            android:name=".VisualizarPedidoGeneralActivity"
            android:exported="false" />
        <activity
            android:name=".VisualizarAtencionesActivity"
            android:exported="false" />
        <activity
            android:name=".ConsumptionHistoryActivity"
            android:exported="false" />
        <activity
            android:name=".RegisterProductActivity"
            android:exported="false" />
        <activity
            android:name=".InfoProductActivity"
            android:exported="false" />
        <activity
            android:name=".RegisterUserActivity"
            android:exported="false" />
        <activity
            android:name=".VisualizarPedidoActivity"
            android:exported="false" />
        <activity
            android:name=".LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:label="@string/app_name"
            android:theme="@style/Theme.Fast_Service.NoActionBar" />
    </application>

</manifest>